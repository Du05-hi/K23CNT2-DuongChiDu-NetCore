@model DuongChiDu_2310900022.Models.DcdEmployee

@{
    ViewData["Title"] = "Xem Thong Tin";
}




    <h4>DcdEmployee</h4>
    <hr />
   





<div class="row">
    <div class="col-md-6">
            <div class="card">
                <div class=" card-header">
                    <h1>@ViewData["Title"]</h1>
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                </div>
                <div class="card-body">
                    <div class="form-group">
                        <label asp-for="DcdEmpId" class="control-label"></label>
                        <input asp-for="DcdEmpId" class="form-control" readonly />

                    </div>
                    <div class="form-group">
                        <label asp-for="DcdEmpName" class="control-label"></label>
                    <input asp-for="DcdEmpName" class="form-control" readonly />
                        <span asp-validation-for="DcdEmpName" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="DcdEmpLevel" class="control-label"></label>
                    <input asp-for="DcdEmpLevel" class="form-control" readonly />
                        <span asp-validation-for="DcdEmpLevel" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="DcdEmpStartDate" class="control-label"></label>
                        <input asp-for="DcdEmpStartDate" class="form-control" readonly />
                        <span asp-validation-for="DcdEmpStartDate" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="DcdEmpStatus" class="control-label"></label>
                        <input asp-for="DcdEmpStatus" class="form-control" readonly />
                        <span asp-validation-for="DcdEmpStatus" class="text-danger"></span>
                    </div>
                </div>
                <div class="card-footer">
                    <div class="form-group">
                       
                    <a asp-action="DcdEdit" asp-route-dcdid="@Model?.DcdEmpId" class="btn btn-primary">Edit</a>
                        <a asp-action="DcdIndex" class="btn btn-secondary">Back to List</a>
                    </div>
                </div>

            </div>





    </div>
</div>

